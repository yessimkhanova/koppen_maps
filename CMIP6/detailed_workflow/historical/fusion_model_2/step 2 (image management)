// // Export the multiband temperature image containing all yearsto your Earth Engine Assets
Export.image.toAsset({
    image: multiBandImageT,
    description: 'temp_historical_fusion_2_all_years',
    assetId: 'users/kalamkas/fusion_2/historical/temp_historical_fusion_2_all_years',
    scale: 27830,
  });
